# Ignore data directories (actual data files)
data/data_files/

# Ignore model data and artifacts (final outputs and experiment records)
models/model_data/

# Ignore __pycache__ directories and compiled Python files
**/__pycache__/
*.py[cod]

# Ignore Jupyter notebook checkpoints
**/.ipynb_checkpoints/

# Ignore virtual environment directories but not .yaml environment files
envs/*
!envs/*.yaml
!envs/*.yml

# Ignore DVC files for tracked data and model outputs
.dvc/
.dvcignore

# Ignore log files and temporary files
logs/
*.log
*.tmp
*.temp

# Ignore unnecessary system files (macOS, Windows)
.DS_Store
Thumbs.db

# Ignore IDE-specific directories (optional)
.vscode/
.idea/
